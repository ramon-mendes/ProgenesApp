{
	"$jason": {
		"head": {
			"@": "mixin.json"
		},
		"body": {
			"header": {
				"title": "[Title goes here]",
				"search": {
					"name": "query_text",
					"placeholder": "Search something",
					"style": {
						"background": "#000000"
					},
					"action": {
						"type": "$util.alert",
						"options": {
							"title": "You've entered:",
							"description": "{{$get.query_text}}"
						}
					}
				}
			},
			"background": {
				"type": "html",
				"url": "https://pacific-fjord-57420.herokuapp.com/",
				"action": {
					"trigger": "handleLink"
				}
			}
		}
	}
}